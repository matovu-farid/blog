<ul class="list-group">
  <%@users.each do |user|%>
  <li class="my-5">
      <%=render 'shared/user', user: user%>
      <%=link_to user_path(id:user.id) ,{:class=>'list-group-item-action p-0 m-0'} do%>
            <button class="btn btn-success ">See More</button>
      <%end%>
  </li>
  <% end %>
</ul>